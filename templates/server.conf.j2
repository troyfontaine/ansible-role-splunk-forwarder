[sslConfig]
{% if splunk_forwarder_output_root_ca_path is defined and splunk_forwarder_output_use_tls %}
sslVerifyServerCert = true
sslRootCAPath = {{ splunk_forwarder_output_root_ca_path is defined and splunk_forwarder_output_use_tls }}
{% endif %}
{% if splunk_forwarder_output_cipher_suite is defined and splunk_forwarder_output_use_tls %}
cipherSuite = {{ splunk_forwarder_output_cipher_suite }}
{% endif %}
{% if splunk_forwarder_output_ssl_version is defined and splunk_forwarder_output_use_tls %}
sslVersionsForClient = {{ splunk_forwarder_output_ssl_version }}
{% endif %}